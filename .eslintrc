{
  "extends": "airbnb",
  "plugins": [
    "react",
    "react-native"
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "import/prefer-default-export": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "jsx-quotes": ["error", "prefer-single"],
    "no-use-before-define": 0,
  }
}
